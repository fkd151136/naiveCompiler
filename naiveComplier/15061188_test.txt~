const int con_a = 0, con_b = +1;
const char con_c1 = '0', con_c2 = 's', con_c3 = '+', con_c4 = '*' ;

int result, var_b[10 * con_b + con_a];
char var_d[9 - con_b / 1], var_c;

int factorial(int n, int no_use)
{
    const int max = 2147483647;
    int ans;
    if (n == 0)
        return (1);
    if (n != 0)
    {
        ans = n * factorial(n - 1);
    }
    return (ans);
}

void getResult(int n)
{
    const int max = 2147483647;
    int a;
    result = factorial(n, 0);
}

int count(int a)
{
    int k;
    k = 0;
    if (a < 0)
        a = -a;
    do
    {
        k = -(-k) + 1;
        a = a / 10;
    }while(a >= 10)

    return (k + 1);
}

